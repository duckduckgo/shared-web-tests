{
    "compilerOptions": {
      "target": "es2021",
      "module": "ESNext",
      "moduleResolution": "node",
      "alwaysStrict": true,
      "allowJs": true,
      "checkJs": true,
      "noEmit": true,
      "noImplicitReturns": true,
      "strictNullChecks": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true
    },
    "exclude": [
      "web-platform-tests",
      "node_modules"
    ]
  }
  